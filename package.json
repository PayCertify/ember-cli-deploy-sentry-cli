{
  "name": "ember-cli-deploy-sentry-cli",
  "version": "1.3.0",
  "description": "Sentry CLI integration for ember-cli-deploy",
  "keywords": [
    "ember-addon",
    "ember-cli-deploy-plugin"
  ],
  "repository": "https://github.com/PayCertify/ember-cli-deploy-sentry-cli",
  "license": "MIT",
  "author": "PayCertify",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "./node_modules/mocha/bin/mocha tests/unit/index-nodetest.js",
    "release": "release-it"
  },
  "release-it": {
    "git": {
      "tagName": "v${version}",
      "commitMessage": "v${version}",
      "pushArgs": "--no-verify"
    },
    "npm": {
      "publish": false
    },
    "github": {
      "release": true
    },
    "hooks": {
      "before:init": "npm run lint && npm test"
    }
  },
  "peerDependencies": {
    "ember-cli-deploy": "^1.0.2"
  },
  "dependencies": {
    "@sentry/cli": "^1.48.0",
    "ember-cli-deploy-plugin": "^0.2.9",
    "ember-cli-deploy-revision-data": "^1.0.0"
  },
  "devDependencies": {
    "@paycertify/eslint-plugin": "github:paycertify/eslint-plugin#v0.16.0",
    "chai": "^4.2.0",
    "eslint": "^6.5.1",
    "mocha": "^8.0.1",
    "release-it": "^14.2.2",
    "sinon": "^9.2.3"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  }
}
